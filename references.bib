	urldate = {2019-06-12},
	journal = {Conservation Biology},
	author = {Brooks, Thomas M. and Mittermeier, Russell A. and Mittermeier, Cristina G. and da Fonseca, Gustavo A. B. and Rylands, Anthony B. and Konstant, William R. and Flick, Penny and Pilgrim, John and Oldfield, Sara and Magin, Georgina and Hilton-Taylor, Craig},
