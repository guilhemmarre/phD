@article{holon_impact_2015,
	title = {The impact of 85 years of coastal development on shallow seagrass beds ({Posidonia} oceanica {L}. ({Delile})) in {South} {Eastern} {France}: {A} slow but steady loss without recovery},
	volume = {165},
	issn = {02727714},
	shorttitle = {The impact of 85 years of coastal development on shallow seagrass beds ({Posidonia} oceanica {L}. ({Delile})) in {South} {Eastern} {France}},
