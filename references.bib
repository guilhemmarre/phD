	file = {Friedlander et al. - 2003 - Designing Effective Marine Protected Areas in Seaf.pdf:C\:\\Users\\Guilhem\\Zotero\\storage\\T2XUSTJD\\Friedlander et al. - 2003 - Designing Effective Marine Protected Areas in Seaf.pdf:application/pdf}
